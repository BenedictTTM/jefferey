
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        String   @id @default(cuid())
  title     String
  excerpt   String
  content   String
  date      DateTime @default(now())

  image     String
  readTime  String   @default("5 min")
}
